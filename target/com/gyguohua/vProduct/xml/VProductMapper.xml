		where id in
		<foreach item="item" collection="array" open="(" separator="," close=")">
			'${item}'
		</foreach>
	</delete>
	
	<update id="update" parameterType="com.gyguohua.model..VProduct">
		update v_product 
		<trim prefix="set" suffixOverrides=",">
			<if test="wrapCode != null and wrapCode != ''">wrap_code = #{wrapCode},</if>
			<if test="wrapName != null and wrapName != ''">wrap_name = #{wrapName},</if>
